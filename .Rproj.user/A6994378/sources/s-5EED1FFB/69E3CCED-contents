cd <- iris %>% select(Sepal.Length)
cd


p =ggplot(cd, aes(cd[,1])) + geom_histogram(fill="gray", color="black") + theme_classic()
library(gridExtra)

grid.arrange(p)


paste("stats_graphs_", strftime(Sys.time(), format = "%Y-%m-%d_%H-%M-%S"), ".pdf", sep = "")
?strftime
?pdf

